	    <apex:includeScript value="{!URLFOR($Resource.iahelp__IHResources, '/js/jsEditHelp.js')}" />

	    <script type='text/javascript'>

			// Prepare page for use
        	function initialise() {
        	
				// Ensure tools buttons adopt IH look and feel for tasks in progress
				initialiseButtons();
				
				return;
        	}
	    
	        // Validation for numeric fields 
	        function checkIsNumber(elem, doFocus, maxValue) {
	            if (isNaN(elem.value)) {
	                alert('Please enter a number between 0 and ' + maxValue);
	                elem.value = 30;
	                elem.select();
	                
	            } else {
	            	if (elem.value > maxValue) { 
		                alert('Please enter a number between 0 and ' + maxValue);
		                elem.value = maxValue;
		                elem.select();
		                
	            	} else {
		                elem.value = parseInt(elem.value, 10);
		                if (doFocus == true) {elem.select()};
	            	}
	            }
	        }

	    </script>
	</head>    

